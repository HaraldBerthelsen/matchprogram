import sys, re


#02	               Filippa Graneld	+	mittföltare	SWE	2001		IFK Lidingö	2019	Djurgårdens IF	25	2



lines = sys.stdin.readlines()
for line in lines:

    #print("#".join(re.split("\t+", line.strip())))

    (nr, name, prev, field, nationality, birthyear, motherclub, tohammarby, fromclub, matches, goals) = re.split("\t+", line.strip())

    if tohammarby == "-":            
        toprint = """
        %s. %s
        %s
        %s %s
        Moderklubb: %s
        Kom: Egen produkt
        Matcher: %s. Mål: %s
        """ % (int(nr), name.strip(), field, nationality, int(birthyear), motherclub, int(matches), int(goals))

    else:
    
        toprint = """
        %s. %s
        %s
        %s %s
        Moderklubb: %s
        Kom: %s (%s)
        Matcher: %s. Mål: %s
        """ % (int(nr), name.strip(), field, nationality, int(birthyear), motherclub, int(tohammarby), fromclub, int(matches), int(goals))

    print(toprint)
    
